APP_ENV=development

# Front-end flags
VITE_GCASH_PAYMENT_MODE=gcash_sandbox

# Backend payment configuration
GCASH_MODE=xendit
XENDIT_SECRET_KEY=
XENDIT_PUBLIC_KEY=
XENDIT_CALLBACK_TOKEN=
XENDIT_MERCHANT_ID=
XENDIT_API_BASE=https://api.xendit.co
# Set to 1 if your amount is already an integer in PHP, or 100 if you charge in cents
XENDIT_AMOUNT_SCALE=100

# Redirect URLs used after the hosted checkout is complete
GCASH_SUCCESS_URL=http://localhost/vitecap1/CAPSTONE1/php/gcash_return.php?status=success
GCASH_CANCEL_URL=http://localhost/vitecap1/CAPSTONE1/php/gcash_return.php?status=cancelled
